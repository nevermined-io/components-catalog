    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "5.2.7",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jest-environment-jsdom": "^27.4.3",
    "jest-mock-axios": "^4.4.1",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-stylelint": "^0.4.2",
    "sass": "^1.43.4",
    "sass-loader": "10.1.1",
