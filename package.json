{
  "name": "@nevermined-io/components-catalog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "npm run clean && npm run build:tsc && npm run copy:scss && npm run build:webpack",
    "build:react": "react-scripts build",
    "build:tsc": "tsc",
    "build:webpack": "webpack --mode production",
    "clean": "rm -rf dist",
    "copy:scss": "cd src/lib && find . -name '*.scss' -exec cp --parents \\{\\} ../../dist/node \\; && cd ../..",
    "test": "react-scripts test --coverage --watchAll=false --silent --env=./test/custom-env.js",
    "eject": "react-scripts eject",
    "ts:lint": "npx eslint . --ext .ts,.tsx"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@nevermined-io/nevermined-sdk-js": "^0.14.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
