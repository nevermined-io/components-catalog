    {
      "title": "Creator",
      "description": "Give proper attribution to the music producers and contributors.",
      "fields": {
        "autonomies.collaborators": {
          "label": "Collaborators",
          "hint": "(Remixers, featured artists, mastering etc)",
          "placeholder": "Anyone else involved in the music creation",
          "type": "text",
          "required": true,
          "maxLength": 25
        },
        "autonomies.label": {
          "label": "Record Label",
          "placeholder": "Name of label (if any)",
          "type": "text",
          "value": "",
          "required": false,
          "fullwidth": true,
          "maxLength": 25
        },
        "autonomies.artworkAuthor": {
          "label": "Artwork Creator",
          "placeholder": "Name of artwork artist/designer",
          "type": "text",
          "value": "",
          "required": false,
          "fullwidth": true,
          "maxLength": 25
        },
        "main.license": {
          "label": "License",
          "type": "input",
          "required": true,
          "value": "No license",
          "hidden": true,
          "options": [
            "Public Domain",
            "PDDL: Public Domain Dedication and License",
            "ODC-By: Attribution License",
            "ODC-ODbL: Open Database License",
            "CDLA-Sharing: Community Data License Agreement",
            "CDLA-Permissive: Community Data License Agreement",
            "CC0: Public Domain Dedication",
            "CC BY: Attribution 4.0 International",
            "CC BY-SA: Attribution-ShareAlike 4.0 International",
            "CC BY-ND: Attribution-NoDerivatives 4.0 International",
            "CC BY-NC: Attribution-NonCommercial 4.0 International",
            "CC BY-NC-SA: Attribution-NonCommercial-ShareAlike 4.0 International",
            "CC BY-NC-ND: Attribution-NonCommercial-NoDerivatives 4.0 International"
          ]
        }
      }
    },
    {
      "title": "Pricing",
      "description": "Set the price, number of editions and how you want to split the revenue from each sale.",
      "fields": {
        "autonomies.fee": {
          "type": "informational-dynamic",
          "price": 1.2,
          "informationText": "Platform fee is"
        },
        "main.price": {
          "label": "Price Per NFT (USDC)",
          "placeholder": "00.00",
          "type": "tel",
          "helperText": "Estimated revenue per NFT:",
          "required": true,
          "fullwidth": false,
          "value": 0
        },
        "autonomies.nftAmount": {
          "label": "NO. of Editions",
          "placeholder": "100",
          "type": "tel",
          "min": 1,
          "value": 1,
          "required": true,
          "fullwidth": false
        },
        "autonomies.recipients": {
          "label": "Split Revenue",
          "description": "Set up anyone as a recipient to split the revenue, just add the Wallet and the split percentage. Please ensure the split totals 100%.",
          "required": true,
          "fields": {
            "autonomies.recipients.name": {
              "label": "Recipient",
              "placeholder": "Name",
              "type": "string",
              "required": true
            },
            "autonomies.recipients.wallet": {
              "label": "Wallet",
              "description": "info will be provided by clem",
              "placeholder": "Address",
              "type": "string",
              "required": true
            },
            "autonomies.recipients.split": {
              "label": "Split %",
              "placeholder": "0",
              "type": "tel",
              "min": 0,
              "max": 100,
              "required": true
            }
          }
        },
        "autonomies.royalties": {
          "label": "Resale Royalty",
          "description": "Set up an royalty for the Creator to receive a share of every secondary resale.",
          "options": [0, 4, 5, 6],
          "fields": {
            "placeholder": "Other %",
            "type": "tel"
          }
        }
      }
    }
