{
  "name": "@nevermined-io/catalog-providers",
  "version": "0.5.2",
  "private": false,
  "scripts": {
    "watch": "./node_modules/.bin/nodemon",
    "start": "npx tsc --watch",
    "build": "npm run clean && npm run build:tsc",
    "build:tsc": "tsc --noEmit false",
    "clean": "rm -rf dist",
    "test:unit": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint ./",
    "lint:fix": "yarn run lint -- --fix",
    "docs": "npx typedoc && rm ../docs/providers/.nojekyll"
  },
  "main": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@wagmi/core": "^0.6.12",
    "connectkit": "^1.0.0",
    "ethers": "^5.6.9",
    "nodemon": "^2.0.16",
    "wagmi": "^0.7.15"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@nevermined-io/nevermined-sdk-js": "^0.25.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^27.0.3",
    "@types/react": "17.0.2",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "eslint": "^8.20.0",
    "eslint-config-nevermined": "^0.2.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eth-testing": "^1.4.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-package-json": "^2.7.0",
    "prettier-stylelint": "^0.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.23.10",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "^4.6.2"
  }
}
