{
    "extends":[
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "react/display-name": "off",
      "react-hooks/exhaustive-deps": "off"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    }
  }